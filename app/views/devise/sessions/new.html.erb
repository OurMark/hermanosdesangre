<div class="col-lg-6">
    <a class="boton_donar" href="quiero_donar.html" title="Quiero donar">
        <%= image_tag "quierodonar_btn.png" %>
    </a>
</div>
<div class="col-lg-6">
  <h3>INGRESAR</h3>
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="form-group">
        <%= f.email_field :email, placeholder: "EMAIL", autofocus: true, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= f.password_field :password, placeholder: "PASSWORD", autocomplete: "off", class: "form-control" %>
      </div>
      <% if devise_mapping.rememberable? -%>
        <%= f.check_box :remember_me %> <%= f.label :remember_me %>
      <% end -%>

        <%= f.submit "Sign in", class: "btn_ingresar" %>
      <% end %>
    </div>
</div>